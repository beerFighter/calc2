{"ast":null,"code":"import HelloWorld from \"@/components/HelloWorld.vue\";\nexport default {\n  components: {\n    HelloWorld\n  },\n  name: 'App',\n\n  data() {\n    return {\n      height: '',\n      width: '',\n      price: '',\n      count: '',\n      array: [],\n      show: true\n    };\n  },\n\n  methods: {\n    createPost() {\n      const newTable = {\n        id: Date.now(),\n        height: this.height,\n        width: this.width,\n        count: this.count,\n        price: this.price,\n        An: this.width * this.height / 100 * this.count * this.price\n      };\n      this.array.push(newTable);\n      this.height = \"\";\n      this.width = \"\";\n      this.count = \"\";\n      this.price = \"\";\n    },\n\n    change() {\n      this.show = !this.show;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8DA,OAAOA,UAAP,MAAuB,6BAAvB;AAEA,eAAe;EACbC,UAAU,EAAE;IACVD;EADU,CADC;EAIbE,IAAI,EAAE,KAJO;;EAKbC,IAAI,GAAG;IACL,OAAO;MACLC,MAAM,EAAE,EADH;MAELC,KAAK,EAAE,EAFF;MAGLC,KAAK,EAAE,EAHF;MAILC,KAAK,EAAE,EAJF;MAKLC,KAAK,EAAE,EALF;MAQLC,IAAI,EAAE;IARD,CAAP;EAUD,CAhBY;;EAiBbC,OAAO,EAAE;IACPC,UAAU,GAAG;MAEX,MAAMC,QAAO,GAAI;QACfC,EAAE,EAAEC,IAAI,CAACC,GAAL,EADW;QAEfX,MAAM,EAAE,KAAKA,MAFE;QAGfC,KAAK,EAAE,KAAKA,KAHG;QAIfE,KAAK,EAAE,KAAKA,KAJG;QAKfD,KAAK,EAAE,KAAKA,KALG;QAMfU,EAAE,EAAK,KAAKX,KAAL,GAAa,KAAKD,MAAnB,GAA6B,GAA9B,GAAsC,KAAKG,KAA5C,GAAqD,KAAKD;MAN/C,CAAjB;MASA,KAAKE,KAAL,CAAWS,IAAX,CAAgBL,QAAhB;MACA,KAAKR,MAAL,GAAc,EAAd;MACA,KAAKC,KAAL,GAAa,EAAb;MACA,KAAKE,KAAL,GAAa,EAAb;MACA,KAAKD,KAAL,GAAa,EAAb;IAED,CAlBM;;IAoBPY,MAAM,GAAG;MACP,KAAKT,IAAL,GAAY,CAAC,KAAKA,IAAlB;IACF;;EAtBO;AAjBI,CAAf","names":["HelloWorld","components","name","data","height","width","price","count","array","show","methods","createPost","newTable","id","Date","now","An","push","change"],"sourceRoot":"","sources":["C:\\Users\\7777\\Desktop\\Calc\\calc\\src\\App.vue"],"sourcesContent":["<template>\n  <div class=\"wrapper page\">\n    <form class=\"calc-form-container\" @submit.prevent :class=\"{show: show === false}\">\n      <div>\n        <div>\n          <input id=\"name\"\n          :value=\"height\"\n          @input=\"height = $event.target.value\"\n          type=\"number\"\n          class=\"input-height\"\n          placeholder=\"Высота в см\"\n          >\n        </div>\n\n        <div >\n          <input \n          :value=\"width\"\n          @input=\"width = $event.target.value\"\n          type=\"number\"\n          class=\"input-width \"\n          placeholder=\"Ширина в см\"\n          >\n        </div>\n        \n        <div >\n          <input \n          :value=\"count\"\n          @input=\"count = $event.target.value\"\n          type=\"number\"\n          class=\"input-count \"\n          placeholder=\"Кол-во\"\n          >\n        </div>\n\n        <div >\n          <input \n          :value=\"price\"\n          @input=\"price = $event.target.value\"\n          type=\"number\"\n          class=\"input-price\"\n          placeholder=\"Цена за ткань m^2\"\n          >\n        </div>\n        <button \n        @click=\"createPost()\"\n        >Добавить</button>\n\n      </div>\n    </form>\n    <hello-world :array=\"array\"/>\n\n    <div>\n      <button @click=\"change()\">\n      Show\n      </button>\n    </div>\n  </div>\n  \n\n</template>\n\n<script>\nimport HelloWorld from \"@/components/HelloWorld.vue\"\n\nexport default {\n  components: {\n    HelloWorld\n  },\n  name: 'App',\n  data() {\n    return {\n      height: '',\n      width: '',\n      price: '',\n      count: '',\n      array: [\n\n      ],\n      show: true,\n    }\n  },\n  methods: {\n    createPost() {\n      \n      const newTable = {\n        id: Date.now(),\n        height: this.height,\n        width: this.width,\n        count: this.count,\n        price: this.price,\n        An: (((this.width * this.height) / 100 ) * this.count) * this.price\n      }\n\n      this.array.push(newTable)\n      this.height = \"\"\n      this.width = \"\"\n      this.count = \"\"\n      this.price = \"\"\n\n    },\n\n    change() {\n      this.show = !this.show\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\">\n\n@import url('https://fonts.googleapis.com/css2?family=Overpass:wght@300&display=swap');\n#app {\n  font-family: 'Overpass', sans-serif;\n}\n.input-height, .input-width, .input-count, .input-price{\n  margin: 10px 5px;\n}\n.show {\n  display: none;\n}\n\n$secondary-color: rgb(0, 0, 0);\n// $main-color: #333;\n// $secondary-color: #2196f3;\n$width: 180px; // Change Me\n\n* {\n  box-sizing: border-box;\n}\n\n// body {background: $main-color;}\n\n.centered {\n  width: $width;\n  height: $width/5;\n  margin: auto;\n  position: absolute;\n  top: 0; bottom: 0;\n  left: 0; right: 0; \n}\n\n.group {\n  width: 100%;\n  height: $width/5;\n  overflow: hidden;\n  position: relative;\n}\n\ninput {\n  display: block;\n  width: 100%;\n  padding-top: $width/15;\n  border: none;\n  border-radius: 0; // For iOS\n  // border-bottom: solid $width/150 rgba(white, .5);\n  color: rgb(0, 0, 0);\n  font-size: $width/15;\n  transition: .3s ease;\n  &:valid {\n    // border-bottom-color: rgba(white, .5);\n    ~label {\n      top: 0;\n      font: 700 $width/25 Roboto;\n      color: rgba(white, .5);\n    }\n  }\n  &:focus {\n    outline: none;\n    // border-bottom-color: $secondary-color;\n    ~label {\n      top: 0;\n      font: 700 $width/25 Roboto;\n      color: $secondary-color;\n    }\n    \n      \n    ~ .bar:before {\n    transform: translateX(0);\n    }\n  }\n\n  // Stop Chrome's hideous pale yellow background on auto-fill\n  \n  &:-webkit-autofill {\n    -webkit-box-shadow: 0 0 0px 1000px inset;\n    -webkit-text-fill-color: white !important;\n    // border-bottom-color: rgba(white, .5);\n  }\n}\n\n.bar {\n  // background: $secondary-color;\n  background: rgba(white, .5);\n  content: '';\n  width: $width;\n  // height: $width/100;\n  height: $width/150;\n  // transform: translateX(-100%);\n  transition: .3s ease;\n  // margin-top: -2px;\n  //\n  position: relative;\n  &:before {\n    content: '';\n    position: absolute;\n    width: 100%;\n    height: 150%;\n    background: $secondary-color;\n    transform: translateX(-100%);\n    \n  }\n}\n\n::selection {background: rgba($secondary-color, .3);}\n</style>\n"]},"metadata":{},"sourceType":"module"}